<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yingchuang.dao.UsersNumMapper">
    <select id="queryUsersNum" resultType="com.yingchuang.entity.UsersNum">
        SELECT * FROM usersnum
    </select>
    
    <update id="updateUsersNum">
        UPDATE usersnum SET usersNum=usersNum+1
    </update>
</mapper>  